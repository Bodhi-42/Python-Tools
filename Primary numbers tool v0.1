# Prime Factorization List

def is_prime_num(n:int): # Prime number or not
    for i in range(2, n):
        if n % i == 0:
            return False
    return True

def prime_factorization(n:int) -> list[int]:
    prime_list = []
    prime_num = 2
    while n // prime_num > 1:
        if is_prime_num(prime_num) and n % prime_num == 0:
            prime_list.append(prime_num)
            n = n // prime_num
        else:
            prime_num += 1
    if n != 1:
        prime_list.append(n)
    return prime_list
        

print(prime_factorization(213161503))
    
